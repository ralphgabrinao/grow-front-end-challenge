{
  "name": "grow-front-end-challenge",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/build/**",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/store/*.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.3.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-minimal-pie-chart": "^3.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "testNoCoverage": "react-scripts test",
    "lint": "eslint '**/*.js'",
    "test": "react-scripts test --env=jsdom --coverage",
    "commit": "npm run lint && npm run test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest-each": "^23.6.0",
    "react-test-renderer": "^16.7.0",
    "redux-saga-test-plan": "^3.7.0",
    "sinon": "^7.2.2"
  }
}
